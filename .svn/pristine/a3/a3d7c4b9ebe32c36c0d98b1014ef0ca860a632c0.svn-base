<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>회원가입</title>
</head>
<body>
    <table width="400" align="center">
        <tr>
            <td align="center"  style="text-decoration: none; font-size: 14px"><b>회원가입</b></td>
        </tr>
    </table>
    <form name="regForm" action="/login/registerProcess.info" method="post">
    <table width="400" align="center">
        <tr>
            <td style="text-decoration: none; font-size: 12px">아이디</td>
            <td><input type="text" id="userId" name="userId" style="width: 250px; height:20px;"></td>
        </tr>
        <tr>
            <td style="text-decoration: none; font-size: 12px">비밀번호</td>
            <td><input type="password" id="password" name="password" style="width: 250px; height:20px;"></td>
        </tr>
        <tr>
            <td style="text-decoration: none; font-size: 12px">이름</td>
            <td><input type="text" id="userName" name="userName" style="width: 250px; height:20px;"></td>
        </tr>
        <tr>
            <td style="text-decoration: none; font-size: 12px">메일주소</td>
            <td><input type="text" id="email" name="email" style="width: 250px; height:20px;"></td>
        </tr>
        <tr>
            <td style="text-decoration: none; font-size: 12px">주소</td>
            <td><input type="text" id="addr" name="addr" style="width: 250px; height:20px;"></td>
        </tr>
        <tr>
            <td style="text-decoration: none; font-size: 12px">전화번호</td>
            <td><input type="text" id="telNum" name="telNum" style="width: 250px; height:20px;"></td>
        </tr>
    </table>
    <table width="400" align="center">
        <tr>
            <td align="center"><input type="submit" id="submit" name="submit" value="회원가입" style="width: 200px; height:30px;"></td>
        </tr>
    </table>
    </form>
</body>
</html>