@charset "utf-8";

/** reset **/
body {font-size:12px;font-family:'dotum'; margin: 0; padding: 0; color: #000;scrollbar-face-color: #fff; scrollbar-shadow-color: #CCC; scrollbar-highlight-color:#fff; scrollbar-3dlight-color: #CCC; scrollbar-darkshadow-color: #fff; scrollbar-track-color: #fff; scrollbar-arrow-color: #999 }
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,form,fieldset,p,button {margin:0;padding:0}
dl, ul, ol, li { list-style:none; padding:0; margin:0 }
h1, h2, h3, h4, h5, h6, p { margin: 0; padding-right: 15px; padding-left: 15px; line-height:180%; }
h1 { font-size:19px; font-weight:bold; color:#333333; }

h3 {  display:block; height:30px;}
h3 span { font-size:12px; font-weight:normal; padding:0px; display:block; margin-top:-15px; text-align:right; }
a { color:#414958; text-decoration:none; cursor:pointer }
a:visited { color: #4E5869;text-decoration: none;}
a:hover, a:active, a:focus {   text-decoration: none; }
a, a:active, a:focus, button { outline: none }
:focus { outline:0 }
img, fieldset, iframe { border:0 none }
img, input, select { font-family:'dotum';vertical-align:middle; font-size:12px; color:#555 }
dl,ul, ol, li {list-style:none; padding:0; margin:0}
textarea {font-family:'dotum';font-size:12px;color:#555}
table {border-collapse:collapse; border-spacing:0; font-size:12px;color:#555 }
form { padding:0; margin:0; border:0 none; font-size:12px }
* html input { margin:-1px 0 }  /* ie6 ie7 input 1px */
*:first-child+html input { margin:-1px 0 } /* ie6 ie7 input 1px */
.text {border:1px solid #B0B0B0;height:18px;line-height:18px;padding-left:2px}
select {height:19px;line-height:19px;margin:-1px 0 0}
textarea {border:1px solid #B0B0B0;padding:2px}



/** common **/
.clear { clear:both }
.tC { text-align:center !important; font-size:12px !important }
.tL { text-align:left !important }
.tR { text-align:right !important }
.fR { float:right !important }
.fL { float:left !important }
.fn { float:none !important }
.fno { font-weight:normal !important }
.vt { vertical-align:top!important }
.vm { vertical-align:middle!important }
.vb { vertical-align:bottom!important }
.bgn { background:none !important }
.bon { border:none !important }
.bbn { border-bottom:none !important }
.over { clear:both; overflow:auto; }
.zoom { zoom:1 }
.wauto { width:auto !important }
.hall { height:100% !important }
.pz { padding:0 !important }
.ptz { padding-top:0 !important }
.mz { margin:0 !important }
.mtz { margin-top:0 !important }
.mtm50 { margin-top:-45px !important }
.mrz { margin-right:0 !important }
.mlz { margin-left:0 !important }
.ml5 { margin-left:5px !important }
.all { width:99% }
.allm { width:96% }
.astro { color:#FF8E00; font-size:12px; margin-right:5px }
.radio { margin-top:-1px; vertical-align:middle}
.overflow {text-align:left;overflow: hidden;text-overflow: ellipsis; white-space:nowrap;-moz-binding: url('../script/ellipsis.xml#ellipsis')}
.overflow a {text-align:left;overflow: hidden; text-overflow: ellipsis; white-space:nowrap;-moz-binding: url('../script/ellipsis.xml#ellipsis')}

